	<!-- Widgets -->
	<widget-groupings>
		<!-- Movies -->
		<node label="$LOCALIZE[342]">
			<shortcut label="$LOCALIZE[31013]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/randommovies.xsp</shortcut>
			<shortcut label="$LOCALIZE[20386]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/recentmovies.xsp</shortcut>
			<shortcut label="$LOCALIZE[31014]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/inprogressmovies.xsp</shortcut>
			<shortcut label="$LOCALIZE[31017]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/recommendedmovies.xsp</shortcut>
		</node>
		<!-- TV Shows -->
