	<background label="$LOCALIZE[15016]" icon="special://skin/backgrounds/games.jpg">special://skin/backgrounds/games.jpg</background>
	<background label="$LOCALIZE[8]" icon="special://skin/backgrounds/weather.jpg">special://skin/backgrounds/weather.jpg</background>
	<background label="$LOCALIZE[1035]" icon="special://skin/backgrounds/favourites.jpg">special://skin/backgrounds/favourites.jpg</background>
