	<property property="backgroundFrame" label="$LOCALIZE[2]" icon="special://skin/backgrounds/music.png">special://skin/backgrounds/music.png</property>
	<!-- Menu -->
	<groupings>
		<node label="32029">
			<content>common</content>
		</node>
		<node label="32030">
			<content>video</content>
			<node label="32040">
				<content>playlist-video</content>
			</node>
			<node label="32010">
				<shortcut label="1037" type="32014" icon="DefaultAddonVideo.png">ActivateWindow(Videos,Addons,return)</shortcut>
				<content>addon-video</content>
			</node>
			<node label="32075">
				<content>videosources</content>
				<content>upnp-video</content>
			</node>
		</node>
		<node label="32031">
			<content>music</content>
			<node label="32040">
				<content>playlist-audio</content>
			</node>
			<node label="32011">
				<shortcut label="1038" type="32019" icon="DefaultAddonMusic.png">ActivateWindow(MusicLibrary,Addons,return)</shortcut>
				<content>addon-audio</content>
			</node>
			<node label="32075">
				<content>musicsources</content>
				<content>upnp-music</content>
			</node>
		</node>
		<node label="14204" condition="PVR.HasTVChannels | PVR.HasRadioChannels">
			<content>pvr</content>
			<node label="19023" condition="PVR.HasTVChannels">
				<content>pvr-tv</content>
			</node>
			<node label="19024" condition="PVR.HasRadioChannels">
				<content>pvr-radio</content>
			</node>
		</node>
		<node label="32020">
			<content>picturesources</content>
		</node>
		<node label="32007">
			<node label="32009">
				<shortcut label="10001" type="32021" icon="DefaultAddonProgram.png">ActivateWindow(Programs,Addons,return)</shortcut>
				<shortcut label="20244" type="32021" icon="DefaultAddonProgram.png" condition="System.Platform.Android">ActivateWindow(Programs,androidapp://sources/apps,return)</shortcut>
				<content>addon-program</content>
			</node>
			<node label="32010">
				<shortcut label="1037" type="32014" icon="DefaultAddonVideo.png">ActivateWindow(Videos,Addons,return)</shortcut>
				<content>addon-video</content>
			</node>
			<node label="32011">
				<shortcut label="1038" type="32019" icon="DefaultAddonMusic.png">ActivateWindow(MusicLibrary,Addons,return)</shortcut>
				<content>addon-audio</content>
			</node>
			<node label="32012">
				<shortcut label="1039" type="32020" icon="DefaultAddonPicture.png">ActivateWindow(Pictures,Addons,return)</shortcut>
				<content>addon-image</content>
			</node>
		</node>
		<node label="32006">
			<shortcut label="1036" type="1036" icon="DefaultFolder.png">ActivateWindow(favouritesbrowser)</shortcut>
			<content>favourite</content>
		</node>
		<node label="10004">
			<content>settings</content>
		</node>
		<node label="32054">
			<content>commands</content>
		</node>
	</groupings>
	<!-- Widgets -->
	<widget-groupings>
		<!-- Movies -->
		<node label="$LOCALIZE[342]">
			<shortcut label="$LOCALIZE[31013]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/randommovies.xsp</shortcut>
			<shortcut label="$LOCALIZE[20386]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/recentmovies.xsp</shortcut>
			<shortcut label="$LOCALIZE[31014]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/inprogressmovies.xsp</shortcut>
			<shortcut label="$LOCALIZE[31017]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(movies)" widget="poster" widgetType="movie" widgetTarget="videos">special://skin/extras/playlists/recommendedmovies.xsp</shortcut>
		</node>
		<!-- TV Shows -->
		<node label="$LOCALIZE[20343]">
			<shortcut label="$LOCALIZE[31015]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="poster" widgetType="tvshow" widgetTarget="videos">special://skin/extras/playlists/randomtvshows.xsp</shortcut>
			<shortcut label="$LOCALIZE[31016]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="poster" widgetType="tvshow" widgetTarget="videos">special://skin/extras/playlists/recenttvshows.xsp</shortcut>
			<shortcut label="$LOCALIZE[626]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="poster" widgetType="tvshow" widgetTarget="videos">special://skin/extras/playlists/inprogresstvshows.xsp</shortcut>
			<shortcut label="$LOCALIZE[31018]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="poster" widgetType="tvshow" widgetTarget="videos">special://skin/extras/playlists/recommendedtvshows.xsp</shortcut>
		</node>
		<!-- Episodes -->
		<node label="$LOCALIZE[20360]">
			<shortcut label="$LOCALIZE[31019]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="landscape" widgetType="episode" widgetTarget="videos">special://skin/extras/playlists/randomepisodes.xsp</shortcut>
			<shortcut label="$LOCALIZE[20387]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="landscape" widgetType="episode" widgetTarget="videos">special://skin/extras/playlists/recentepisodes.xsp</shortcut>
			<shortcut label="$LOCALIZE[31020]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="landscape" widgetType="episode" widgetTarget="videos">special://skin/extras/playlists/inprogressepisodes.xsp</shortcut>
			<shortcut label="$LOCALIZE[31021]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="landscape" widgetType="episode" widgetTarget="videos">special://skin/extras/playlists/recommendedepisodes.xsp</shortcut>
		</node>
		<!-- Music Videos -->
		<node label="$LOCALIZE[20389]">
			<shortcut label="$LOCALIZE[31022]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(musicvideos)" widget="landscape" widgetType="musicvideo" widgetTarget="videos">special://skin/extras/playlists/randommusicvideos.xsp</shortcut>
			<shortcut label="$LOCALIZE[20390]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(musicvideos)" widget="landscape" widgetType="musicvideo" widgetTarget="videos">special://skin/extras/playlists/recentmusicvideos.xsp</shortcut>
			<shortcut label="$LOCALIZE[31023]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(musicvideos)" widget="landscape" widgetType="musicvideo" widgetTarget="videos">special://skin/extras/playlists/recommendedmusicvideos.xsp</shortcut>
		</node>
		<!-- Albums -->
		<node label="$LOCALIZE[132]">
			<shortcut label="$LOCALIZE[31028]" icon="DefaultMusicPlaylists.png" condition="Library.HasContent(music)" widget="square" widgetType="album" widgetTarget="music">special://skin/extras/playlists/randomalbums.xsp</shortcut>
			<shortcut label="$LOCALIZE[359]" icon="DefaultMusicPlaylists.png" condition="Library.HasContent(music)" widget="square" widgetType="album" widgetTarget="music">special://skin/extras/playlists/recentlyaddedalbums.xsp</shortcut>
			<shortcut label="$LOCALIZE[31029]" icon="DefaultMusicPlaylists.png" condition="Library.HasContent(music)" widget="square" widgetType="album" widgetTarget="music">special://skin/extras/playlists/recommendedalbums.xsp</shortcut>
		</node>
		<!-- System Info -->
		<shortcut label="$LOCALIZE[138]" icon="homeinfo-system.png" widget="systeminfo">systeminfo</shortcut>
		<!-- Weather Info -->
		<shortcut label="$LOCALIZE[20088]" icon="DefaultAddonWeather.png" widget="weatherinfo">weatherinfo</shortcut>
		<!-- Recordings Info -->
		<shortcut label="$LOCALIZE[10601]" icon="homeinfo-pvrtimer.png" widget="recordinginfo">recordinginfo</shortcut>
	</widget-groupings>
	<!-- Default Widgets -->
	<widgetdefaultnode defaultID="movies" label="$LOCALIZE[20386]" type="movie" target="videos" path="special://skin/extras/playlists/recentmovies.xsp">poster</widgetdefaultnode>
	<widgetdefaultnode defaultID="tvshows" label="$LOCALIZE[31016]" type="tvshow" target="videos" path="special://skin/extras/playlists/recenttvshows.xsp">poster</widgetdefaultnode>
	<widgetdefaultnode defaultID="music" label="$LOCALIZE[359]" type="album" target="music" path="special://skin/extras/playlists/recentlyaddedalbums.xsp">square</widgetdefaultnode>
	<!-- Remove unneeded menu properties -->
	<propertySettings property="defaultID" templateonly="True" />
	<propertySettings property="labelID" templateonly="True" />
	<propertySettings property="id" templateonly="True" />
	<propertySettings property="group" templateonly="True" />
	<propertySettings property="list" templateonly="True" />
	<propertySettings property="widget" templateonly="True" />
	<propertySettings property="widgetTarget" templateonly="True" />
	<propertySettings property="widgetType" templateonly="True" />
	<propertySettings property="widgetPath" templateonly="True" />
</overrides>