			<shortcut label="$LOCALIZE[31021]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(tvshows)" widget="landscape" widgetType="episode" widgetTarget="videos">special://skin/extras/playlists/recommendedepisodes.xsp</shortcut>
		</node>
		<!-- Music Videos -->
		<node label="$LOCALIZE[20389]">
			<shortcut label="$LOCALIZE[31022]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(musicvideos)" widget="landscape" widgetType="musicvideo" widgetTarget="videos">special://skin/extras/playlists/randommusicvideos.xsp</shortcut>
			<shortcut label="$LOCALIZE[20390]" icon="DefaultVideoPlaylists.png" condition="Library.HasContent(musicvideos)" widget="landscape" widgetType="musicvideo" widgetTarget="videos">special://skin/extras/playlists/recentmusicvideos.xsp</shortcut>
