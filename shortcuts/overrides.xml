	<backgrounddefault defaultID="movies">special://skin/backgrounds/movies.jpg</backgrounddefault>
	<backgrounddefault defaultID="tvshows">special://skin/backgrounds/tvshows.jpg</backgrounddefault>
	<backgrounddefault defaultID="musicvideos">special://skin/backgrounds/musicvideos.jpg</backgrounddefault>
	<backgrounddefault defaultID="tv">special://skin/backgrounds/tv.jpg</backgrounddefault>
