		</node>
		<!-- Albums -->
		<node label="$LOCALIZE[132]">
			<shortcut label="$LOCALIZE[31028]" icon="DefaultMusicPlaylists.png" condition="Library.HasContent(music)" widget="square" widgetType="album" widgetTarget="music">special://skin/extras/playlists/randomalbums.xsp</shortcut>
			<shortcut label="$LOCALIZE[359]" icon="DefaultMusicPlaylists.png" condition="Library.HasContent(music)" widget="square" widgetType="album" widgetTarget="music">special://skin/extras/playlists/recentlyaddedalbums.xsp</shortcut>
			<shortcut label="$LOCALIZE[31029]" icon="DefaultMusicPlaylists.png" condition="Library.HasContent(music)" widget="square" widgetType="album" widgetTarget="music">special://skin/extras/playlists/recommendedalbums.xsp</shortcut>
		</node>
		<!-- System Info -->
		<shortcut label="$LOCALIZE[138]" icon="homeinfo-system.png" widget="systeminfo">systeminfo</shortcut>
		<!-- Weather Info -->
		<shortcut label="$LOCALIZE[20088]" icon="DefaultAddonWeather.png" widget="weatherinfo">weatherinfo</shortcut>
		<!-- Recordings Info -->
		<shortcut label="$LOCALIZE[10601]" icon="homeinfo-pvrtimer.png" widget="recordinginfo">recordinginfo</shortcut>
	</widget-groupings>
	<!-- Default Widgets -->
	<widgetdefaultnode defaultID="movies" label="$LOCALIZE[20386]" type="movie" target="videos" path="special://skin/extras/playlists/recentmovies.xsp" >poster</widgetdefaultnode>
	<widgetdefaultnode defaultID="tvshows" label="$LOCALIZE[31016]" type="tvshow" target="videos" path="special://skin/extras/playlists/recenttvshows.xsp" >poster</widgetdefaultnode>
	<widgetdefaultnode defaultID="music" label="$LOCALIZE[359]" type="album" target="music" path="special://skin/extras/playlists/recentlyaddedalbums.xsp" >square</widgetdefaultnode>
</overrides>